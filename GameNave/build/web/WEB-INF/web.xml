<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>servlets.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registrar</servlet-name>
        <servlet-class>servlets.registrar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>salir</servlet-name>
        <servlet-class>servlets.salir</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>mispartidas</servlet-name>
        <servlet-class>servlets.mispartidas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>top10</servlet-name>
        <servlet-class>servlets.top10</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>topviciados</servlet-name>
        <servlet-class>servlets.topviciados</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>enlinea</servlet-name>
        <servlet-class>servlets.enlinea</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registrar</servlet-name>
        <url-pattern>/registrar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>salir</servlet-name>
        <url-pattern>/salir</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>mispartidas</servlet-name>
        <url-pattern>/mispartidas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>top10</servlet-name>
        <url-pattern>/top10</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>topviciados</servlet-name>
        <url-pattern>/topviciados</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>enlinea</servlet-name>
        <url-pattern>/enlinea</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>lander/index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
